    </collision>
    <collision>
      <origin xyz="0 1 1"/>
      <geometry>
        <plane normal="0 -1 0"/>
      </geometry>
    </collision>
  </link>

  <joint name="floor_to_wall" type="fixed">
    <parent link="floor"/>
    <child link="wall"/>
  </joint>

  <link name="floating_base_dummy">
    <inertial>
      <mass value="0.0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
  </link>

  <joint name="floating_base" type="planar">
    <axis xyz="0 -1 0"/>
    <origin rpy="0 0 1.5707963267948966"/>
    <parent link="root"/>
    <child link="floating_base_dummy"/>
  </joint>

  <link name="pelvis">
