<?xml version="1.0"?>

<robot name="BoxAtlas">
  <link name="root">
  </link>

  <link name="floor">
    <visual>
      <origin xyz="0 0.5 -0.05"/>
      <geometry>
        <box size="1 3 0.1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0.5 -0.05"/>
      <geometry>
        <box size="1 3 0.1"/>
      </geometry>
    </collision>
    <collision>
      <geometry>
        <plane normal="0 0 1"/>
      </geometry>
    </collision>
  </link>

  <joint name="root_to_floor" type="fixed">
    <parent link="root"/>
    <child link="floor"/>
  </joint>

  <link name="wall">
    <visual>
      <origin xyz="0 1.05 1"/>
